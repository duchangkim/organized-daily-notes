{
  "name": "organized-daily-notes",
  "version": "1.0.0",
  "description": "Automatically organizes your daily notes into customizable folder structures (Year/Month/Week) for enhanced organization and easier navigation",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "obsidian plugin"
  ],
  "author": "duchangkim",
  "license": "MIT",
  "devDependencies": {
    "@duchi/prettier-config": "^1.0.10",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  },
  "prettier": "@duchi/prettier-config"
}
